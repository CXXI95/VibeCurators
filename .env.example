# VibeLens Environment Variables
# Copy this file to .env and fill in actual values

# === AWS RDS PostgreSQL Configuration ===
# Get these from AWS RDS Console after creating database
PG_HOST=your-database.xxxxxxxxxx.us-west-2.rds.amazonaws.com
PG_PORT=5432
PG_DATABASE=vibelens
PG_USER=postgres
PG_PASSWORD=your-master-password

# === AWS S3 Configuration (Optional) ===
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-west-2
S3_BUCKET_NAME=vibelens-data

# === Model Configuration ===
EMBEDDING_MODEL=all-MiniLM-L6-v2
EMBEDDING_DIM=384

# === ETL Parameters ===
MIN_RATINGS_THRESHOLD=100
SPARK_DRIVER_MEMORY=8g
SPARK_EXECUTOR_MEMORY=8g
